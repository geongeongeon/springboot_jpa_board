DROP DATABASE IF EXISTS jpa_board;
CREATE DATABASE jpa_board;

USE jpa_board;

SHOW tables;

DESC site_user;
SELECT * FROM site_user;

DESC post;
SELECT * FROM post;

DESC answer;
SELECT * FROM answer;

DESC post_liked_users;
SELECT * FROM post_liked_users;

DESC site_user_like_posts;
SELECT * FROM site_user_like_posts;